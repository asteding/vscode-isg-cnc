{
  "scopeName": "source.isg-cnc",
  "name": "VSCode ISG-CNC Syntax",
  "patterns": [
    {
      "include": "#comments"
    },
    {
      "include": "#speedsfeeds"
    },
    {
      "include": "#prognames"
    },
    {
      "include": "#coords"
    },
    {
      "include": "#tools"
    },
    {
      "include": "#modifiers"
    },
    {
      "include": "#variables"
    },
    {
      "include": "#gcodes"
    },
    {
      "include": "#mcodes"
    },
    {
      "include": "#dollarkeywords"
    },
    {
      "include": "#hashtagkeywords"
    },
    {
      "include": "#operators"
    },
    {
      "include": "#constants"
    },
    {
      "include": "#programcalls"
    },
    {
      "include": "#common"
    }
  ],
  "repository": {
    "comments": {
      "patterns": [
        {
          "name": "comment.documentation",
          "begin": "\\#COMMENT\\sBEGIN",
          "end": "\\#COMMENT\\sEND"
        },
        {
          "name": "comment",
          "begin": "\\(",
          "end": "\n"
        },
        {
          "name": "comment",
          "begin": "\\(",
          "end": "\\)"
        },
        {
          "name": "comment",
          "match": "(\\s?);.*"
        }
      ]
    },
    "gcodes": {
      "patterns": [
        {
          "match": "\\b[Gg][0-9]{1,3}\\b",
          "name": "isg-cnc.keyword.gcode"
        }
      ]
    },
    "mcodes": {
      "patterns": [
        {
          "match": "\\b[Mm][0-9]{1,3}\\b",
          "name": "isg-cnc.keyword.mcode"
        }
      ]
    },
    "dollarkeywords": {
      "patterns": [
        {
          "match": "\\$BREAK",
          "name": "isg-cnc.keyword.dollar"
        },
        {
          "match": "\\$CONTINUE",
          "name": "isg-cnc.keyword.dollar"
        },
        {
          "match": "\\$DO",
          "name": "isg-cnc.keyword.dollar"
        },
        {
          "match": "\\$ENDDO",
          "name": "isg-cnc.keyword.dollar"
        },
        {
          "match": "\\$REPEAT",
          "name": "isg-cnc.keyword.dollar"
        },
        {
          "match": "\\$UNTIL",
          "name": "isg-cnc.keyword.dollar"
        },
        {
          "match": "\\$FOR",
          "name": "isg-cnc.keyword.dollar"
        },
        {
          "match": "\\$ENDFOR",
          "name": "isg-cnc.keyword.dollar"
        },
        {
          "match": "\\$GOTO",
          "name": "isg-cnc.keyword.dollar"
        },
        {
          "match": "\\$IF",
          "name": "isg-cnc.keyword.dollar"
        },
        {
          "match": "\\$ELSEIF",
          "name": "isg-cnc.keyword.dollar"
        },
        {
          "match": "\\$ENDIF",
          "name": "isg-cnc.keyword.dollar"
        },
        {
          "match": "\\$ELSE",
          "name": "isg-cnc.keyword.dollar"
        },
        {
          "match": "\\$SWITCH",
          "name": "isg-cnc.keyword.dollar"
        },
        {
          "match": "\\$CASE",
          "name": "isg-cnc.keyword.dollar"
        },
        {
          "match": "\\$DEFAULT",
          "name": "isg-cnc.keyword.dollar"
        },
        {
          "match": "\\$ENDSWITCH",
          "name": "isg-cnc.keyword.dollar"
        },
        {
          "match": "\\$WHILE",
          "name": "isg-cnc.keyword.dollar"
        },
        {
          "match": "\\$ENDWHILE",
          "name": "isg-cnc.keyword.dollar"
        }
      ]
    },
    "hashtagkeywords": {
      "patterns": [
        {
          "match": "\\#GANTRY\\s((ON\\sALL)|ON\\s?\\[.*\\]|(OFF\\sALL)|OFF\\s?\\[.*\\])",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#ACHSE",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#ACHSTAUSCH",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#ACS\\s((DEF\\sACT)|DEF)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#ACS\\s(ON|(OFF\\sALL)|OFF)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#ADD",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#AKIMA\\s(STARTVECTOR|ENDVECTOR|TRANS)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#ANG",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#AX\\s((DEF\\sDEFAULT)|DEF)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#AX\\s(LINK\\s(ON|OFF|ALL)|REQUEST|((RELEASE\\sALL)|(UNLOCK\\sALL)|RELEASE)|LOCK)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#BACKWARD\\sSTORAGE\\sCLEAR",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#BLOCKSEARCH\\s(LOCKED|RELEASED)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#CACHE\\s(CLEAR|LOAD)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#CALL\\sAX",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#CAXTRACK\\s(ON|OFF)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "(\\#CAX\\s?(OFF|ON|(\\[(-?(?:\\d+(?:\\.\\d*)?))\\])))|#CAX",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#CHANNEL\\s(INIT|SET|(INTERFACE\\s(ON|OFF)))",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#CHR",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#CHF",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#CLEAR\\sCONFIG",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#COMMAND\\s((WR\\sSYN|WR)|(WAIT\\sSYN|WAIT))",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "(\\#CONTOUR\\s(MODE\\sON|MODE))",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#CONTROL\\sAREA\\s(BEGIN|END|OFF|ON|CLEAR)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#CORNER\\sPARAM",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#CYL\\sORI\\s(LATERAL|PROFILE)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#CS\\s((ADD|(DEF\\sACT)|DEF|(DEL\\sALL)|MODE\\s(ON|OFF))|ON|(OFF\\sALL)|OFF|SELECT)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "(\\#CYL\\s?(OFF|(\\[(-?(?:\\d+(?:\\.\\d*)?))\\])))|\\#CYL",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#DELETE",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#DEL\\sDIST2GO",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#DISTANCE\\sPROG\\sSTART\\s(ON|OFF|CLEAR)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#DRIVE\\s(WR|WAIT)\\sSYN",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#DYN_WEIGHT\\s(ON|OFF)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#DYNAMIC\\sWEIGHT\\s(ON|OFF)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#ECS\\s(ON|OFF)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#EDM\\s(ON|OFF)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#EDGE\\sMACHINING\\s(ON\\sDEFAULT|ON|OFF)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "(\\#DISABLE|\\#ENABLE)\\sAX\\sLINK",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#ERROR",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#EXPORT\\sVE",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#EXPL\\sSYN",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "(\\#FACE\\s?(OFF|\\[(-?(?:\\d+(?:\\.\\d*)?))\\])|\\#FACE)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "(\\#FGROUP\\s(ROT|WAXIS)|\\#FGROUP)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#FILE\\s(NAME|EXIST|DELETE|RENAME)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "(\\#FILTER\\s(ON|OFF)|\\#FILTER|\\s\\[HSC\\])",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "(\\#FLUSH\\s(CONTINUE|WAIT)|\\#FLUSH)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#FRC",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#FREE\\sTOOL\\sCHANGE\\s(ON|OFF)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#GET\\s((CMDPOS|ACTPOS)|MANUAL\\sOFFSETS|WCS\\sPOSLIMIT)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#HANDWHEEL",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#HSC\\s(ON|OFF)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#IDENT\\s((WR|RD)\\sSYN|(WR|RD))",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#INIT\\sMACRO\\sTAB",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#INIT\\sV\\.E\\.",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#JOG\\s(CONT|INCR)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#KIN\\sID",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#LOAD\\sCONFIG",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#MACHINE\\sDATA",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#MAIN\\sSPINDLE",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#MANUAL\\sLIMITS",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#MCS\\s(ON\\sEX\\sTOOL|ON|OFF|TO\\sWCS)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "(\\#MEAS\\s(MODE|DEFAULT)|\\#MEAS)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#MICROJOINT",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "(\\#MSG\\s(INFO|SAVE\\sEXCLUSIVE|SAVE|(SYN\\sACK|SYN))|\\#MSG)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#NIBBLE\\s(ON|OFF)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#OPTIONAL\\sEXECUTION\\s(ON|OFF)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "(\\#OTC\\s(ON|OFF)|\\#OTC)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#OVERRIDE",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#PSET",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#PRESET",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#PTP\\s(ON|OFF)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#PUNCH\\s(ON|OFF)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#PUT\\s(AX\\sALL|AX)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#RND",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#ROTATION\\s(ON|OFF)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#RTCP\\s(ON|OFF)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#SAVE\\sCONFIG",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "(\\#SCALE\\s(ON|OFF)|\\#SCALE)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#SEGMENTATION\\s(ON\\sALL|ON|OFF\\sALL|OFF)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#SET\\s(SLOPE\\sPROFIL|(AX\\sLINK|AX))",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#SINGLE\\sSTEP|\\s(\\[ENABLE\\]|\\[DISABLE\\]|\\[RESOLUTION\\s?\\=\\s?((-?(?:\\d+(?:\\.\\d*)?)))?\\])",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#SPLINE\\s(ON|OFF|TYPE\\sAKIMA|TYPE\\sBSPLINE)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "(\\#SIGNAL\\sREMOVE|\\#SIGNAL)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "(\\#SLOPE\\sDEFAULT|\\#SLOPE)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#SUPPRESS\\sOFFSETS",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#STROKE\\sDEF\\s(BEGIN|END)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#TANGFEED",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#TIMER",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#TIME",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#TLC\\s(ON|OFF)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#TOOL\\s(DATA|LIFE\\sREAD|LIFE\\sREMOVE|ORI\\sCS|PREP|REFRESH)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#TRACK\\sCS\\s(ON|OFF|ABS)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "(\\#TRAFO\\s(ON|OFF)|\\#TRAFO)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#TRANSVELMIN\\s(ON|OFF)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#TRC",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#VAR",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#ENDVAR",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#VECTOR\\sLIMIT\\s(ON\\sALL|ON|OFF\\sALL|OFF)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "(\\#VOLCOMP\\s(ON|OFF)|\\#VOLCOMP)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "(\\#WAIT\\s(FOR|INDP\\sALL|INDP)|\\#WAIT)",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#WCS\\sTO\\sMCS",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#INFO",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#IF",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#ELSE",
          "name": "isg-cnc.keyword.hashtag"
        },
        {
          "match": "\\#ENDIF",
          "name": "isg-cnc.keyword.hashtag"
        }
      ]
    },
    "operators": {
      "patterns": [
        {
          "match": "(\\+)|(\\-)|(\\*)|(\\/)|(\\*\\*)|(\\sMOD\\s)",
          "name": "isg-cnc.keyword.operator"
        },
        {
          "match": "(ABS)\\s*\\[(-?(?:\\d+(?:\\.\\d*)?))\\]|(SQR)\\s*\\[(-?(?:\\d+(?:\\.\\d*)?))\\]|(SQRT)\\s*\\[(-?(?:\\d+(?:\\.\\d*)?))\\]|(EXP)\\s*\\[(-?(?:\\d+(?:\\.\\d*)?))\\]|(LN)\\s*\\[(-?(?:\\d+(?:\\.\\d*)?))\\]|(DEXP)\\s*\\[(-?(?:\\d+(?:\\.\\d*)?))\\]|(LOG)\\s*\\[(-?(?:\\d+(?:\\.\\d*)?))\\]",
          "name": "isg-cnc.keyword.operator"
        },
        {
          "match": "(\\&)|(\\|)|(\\^)|(INV)\\s*\\[(-?(?:\\d+(?:\\.\\d*)?))\\]",
          "name": "isg-cnc.keyword.operator"
        },
        {
          "match": "(\\&\\&)|(\\|\\|)|(XOR)|(NOT)\\s*\\[(-?(?:\\d+(?:\\.\\d*)?))\\]",
          "name": "isg-cnc.keyword.operator"
        },
        {
          "match": "(SIN)\\s*\\[(-?(?:\\d+(?:\\.\\d*)?))\\]|(COS)\\[(-?(?:\\d+(?:\\.\\d*)?))\\]|(TAN)\\s*\\[(-?(?:\\d+(?:\\.\\d*)?))\\]|(COT)\\s*\\[(-?(?:\\d+(?:\\.\\d*)?))\\]|(ASIN)\\s*\\[(-?(?:\\d+(?:\\.\\d*)?))\\]|(ACOS)\\s*\\[(-?(?:\\d+(?:\\.\\d*)?))\\]|(ATAN)\\s*\\[(-?(?:\\d+(?:\\.\\d*)?))\\]|(ATAN2)\\s*\\[(-?(?:\\d+(?:\\.\\d*)?))\\]|(ACOT)\\s*\\[(-?(?:\\d+(?:\\.\\d*)?))\\]",
          "name": "isg-cnc.keyword.operator"
        },
        {
          "match": "(INT)\\s*\\[(-?(?:\\d+(?:\\.\\d*)?))\\]|(FRACT)\\s*\\[(-?(?:\\d+(?:\\.\\d*)?))\\]|(CEIL)\\s*\\[(-?(?:\\d+(?:\\.\\d*)?))\\]|(ROUND)\\s*\\[(-?(?:\\d+(?:\\.\\d*)?))\\]|(FLOOR)\\s*\\[(-?(?:\\d+(?:\\.\\d*)?))\\]",
          "name": "isg-cnc.keyword.operator"
        },
        {
          "match": "(==)|(!=)|(>=)|(<=)|(<)|(>)",
          "name": "isg-cnc.keyword.operator"
        },
        {
          "match": "(TRUE)|(FALSE)",
          "name": "isg-cnc.keyword.operator"
        }
      ]
    },
    "constants": {
      "patterns": [
        {
          "match": "(PI)",
          "name": "isg-cnc.keyword.constant"
        }
      ]
    },
    "speedsfeeds": {
      "patterns": [
        {
          "match": "\\b(@?[Ss])((?=P[0-9]+)|(?=V\\.)|(-?(?:\\d+(?:\\.\\d*)?)))",
          "name": "isg-cnc.keyword.technology"
        },
        {
          "match": "\\b[Ff]((?=P[0-9]+)|(?=V\\.)|(-?(?:\\d+(?:\\.\\d*)?)))",
          "name": "isg-cnc.keyword.technology"
        },
        {
          "match": "\\b[Ee]((?=P[0-9]+)|(?=V\\.)|(-?(?:\\d+(?:\\.\\d*)?)))",
          "name": "isg-cnc.keyword.technology"
        },
        {
          "match": "\\b[Hh]((?=P[0-9]+)|(?=V\\.)|(-?(?:\\d+(?:\\.\\d*)?)))",
          "name": "isg-cnc.keyword.technology"
        }
      ]
    },
    "prognames": {
      "patterns": [
        {
          "match": "^%(-?(?:\\d+(?:\\.\\d*)?))\n",
          "name": "isg-cnc.programname"
        },
        {
          "match": "^[Oo](-?(?:\\d+(?:\\.\\d*)?))\n",
          "name": "isg-cnc.programname"
        }
      ]
    },
    "common": {
      "patterns": [
        {
          "match": "\\b[Nn][0-9]+",
          "name": "isg-cnc.blocknumber"
        }
      ]
    },
    "coords": {
      "patterns": [
        {
          "match": "\\b(@?X)((?=P[0-9]+)|(?=V\\.)|(-?(?:\\d+(?:\\.\\d*)?))|(?=\\[.+\\]))",
          "name": "isg-cnc.axis"
        },
        {
          "match": "\\b(@?Y)((?=P[0-9]+)|(?=V\\.)|(-?(?:\\d+(?:\\.\\d*)?))|(?=\\[.+\\]))",
          "name": "isg-cnc.axis"
        },
        {
          "match": "\\b(@?Z)((?=P[0-9]+)|(?=V\\.)|(-?(?:\\d+(?:\\.\\d*)?))|(?=\\[.+\\]))",
          "name": "isg-cnc.axis"
        },
        {
          "match": "\\b(@?A)((?=P[0-9]+)|(?=V\\.)|(-?(?:\\d+(?:\\.\\d*)?))|(?=\\[.+\\]))",
          "name": "isg-cnc.axis"
        },
        {
          "match": "\\b(@?B)((?=P[0-9]+)|(?=V\\.)|(-?(?:\\d+(?:\\.\\d*)?))|(?=\\[.+\\]))",
          "name": "isg-cnc.axis"
        },
